<Window x:Class="Первый_проект_за_год_xddddddd.Windows.CountryEditWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Первый_проект_за_год_xddddddd.Windows"
        mc:Ignorable="d"
        Title="CountryEditWin" Height="250" Width="400" Icon="imgs/Лого.jpg" Style="{StaticResource StyleWin}">
    <Grid>
        <Viewbox Margin="10">
            <StackPanel Width="300" HorizontalAlignment="Center" VerticalAlignment="Center">
                <Label>Страна</Label>
                <TextBox Height="30" Text="{Binding Name}"></TextBox>
                
                <Button Content="Сохранить" Click="bt_SaveClick" Margin="0, 10"/>
            </StackPanel>
        </Viewbox>
    </Grid>
</Window>
